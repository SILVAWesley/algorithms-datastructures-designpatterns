{
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended",
        "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": ["@typescript-eslint", "eslint-plugin-tsdoc","prettier"],
    "rules": {
      "tsdoc/syntax": "warn",
      "prettier/prettier": "error"
    }
}
